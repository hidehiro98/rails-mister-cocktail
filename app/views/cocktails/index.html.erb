<h1>Cocktails#index</h1>

<table>
  <% @cocktails.each do |cocktail| %>
    <tr>
      <td><%= link_to cocktail.name, cocktail_path(cocktail) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Create new cocktail', new_cocktail_path, { class: 'btn btn-primary' } %>

